<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/rpg#"
     xml:base="http://example.org/rpg"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://example.org/rpg"/>

    <owl:Class rdf:about="http://example.org/rpg#LivingBeing"/>

    <owl:Class rdf:about="http://example.org/rpg#Hero">
        <rdfs:subClassOf rdf:resource="http://example.org/rpg#LivingBeing"/>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/rpg#Monster">
        <rdfs:subClassOf rdf:resource="http://example.org/rpg#LivingBeing"/>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/rpg#Item"/>

    <owl:Class rdf:about="http://example.org/rpg#Weapon">
        <rdfs:subClassOf rdf:resource="http://example.org/rpg#Item"/>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/rpg#Location"/>

    <owl:ObjectProperty rdf:about="http://example.org/rpg#possesses">
        <rdfs:domain rdf:resource="http://example.org/rpg#Hero"/>
        <rdfs:range rdf:resource="http://example.org/rpg#Item"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/rpg#livesIn">
        <rdfs:domain rdf:resource="http://example.org/rpg#LivingBeing"/>
        <rdfs:range rdf:resource="http://example.org/rpg#Location"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/rpg#isEnemyOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://example.org/rpg#LivingBeing"/>
        <rdfs:range rdf:resource="http://example.org/rpg#LivingBeing"/>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/rpg#healthPoints">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/rpg#name">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:NamedIndividual rdf:about="http://example.org/rpg#DarkForest">
        <rdf:type rdf:resource="http://example.org/rpg#Location"/>
        <name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Forest of Shadows</name>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/rpg#RoyalCastle">
        <rdf:type rdf:resource="http://example.org/rpg#Location"/>
        <name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Camelot Castle</name>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/rpg#Excalibur">
        <rdf:type rdf:resource="http://example.org/rpg#Weapon"/>
        <name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Excalibur</name>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/rpg#MagicStaff">
        <rdf:type rdf:resource="http://example.org/rpg#Weapon"/>
        <name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Frost Staff</name>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/rpg#Arthur">
        <rdf:type rdf:resource="http://example.org/rpg#Hero"/>
        <name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">King Arthur</name>
        <healthPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">100</healthPoints>
        <possesses rdf:resource="http://example.org/rpg#Excalibur"/>
        <livesIn rdf:resource="http://example.org/rpg#RoyalCastle"/>
        <isEnemyOf rdf:resource="http://example.org/rpg#Mordred"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/rpg#Merlin">
        <rdf:type rdf:resource="http://example.org/rpg#Hero"/>
        <name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Merlin the Enchanter</name>
        <healthPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">80</healthPoints>
        <possesses rdf:resource="http://example.org/rpg#MagicStaff"/>
        <livesIn rdf:resource="http://example.org/rpg#RoyalCastle"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/rpg#Mordred">
        <rdf:type rdf:resource="http://example.org/rpg#Monster"/>
        <name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mordred the Traitor</name>
        <healthPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">120</healthPoints>
        <livesIn rdf:resource="http://example.org/rpg#DarkForest"/>
        <isEnemyOf rdf:resource="http://example.org/rpg#Arthur"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/rpg#Goblin01">
        <rdf:type rdf:resource="http://example.org/rpg#Monster"/>
        <name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Snaga</name>
        <livesIn rdf:resource="http://example.org/rpg#DarkForest"/>
    </owl:NamedIndividual>

</rdf:RDF>